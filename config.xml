<?xml version='1.0' encoding='utf-8'?>
<widget id="ec.com.laberintos.appx" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LaberintosApp</name>
    <description>
        Laberintos Vivero Jard√≠n APP
    </description>
    <author email="vivero@laberintos.com.ec" href="http://www.laberintos.com.ec">
        laberintos.com.ec
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="data:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-splashscreen" spec="~5.0.2" />
    <icon src="img/icon.png" />
    <splash platform="android" src="img/splash.png" />
    <splash platform="ios" src="img/splash.png" />
    <preference name="SplashScreen" value="img/splash.png" />
    <preference name="SplashScreenDelay" value="4000" />
    <preference name="SplashScreenBackgroundColor" value="#95CA17" />
    <preference name="SplashScreenSpinnerColor" value="white" />
    <engine name="android" spec="~7.0.0" />
    <engine name="ios" spec="~4.5.4" />
    <engine name="browser" spec="^5.0.3" />
</widget>
